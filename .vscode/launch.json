	"version": "0.2.0",
	"configurations": [
		{
			"name": "WSL - Build",
			"type": "f5anything",
			"request": "launch",
			"command": "wsl make kernel",
			"terminalName": "F5 Anything",
			"terminalIndex": -1,
			"showTerminal": true,
			"vscCommand": ""
		}, {
			"name": "WSL - ISO",
			"type": "f5anything",
			"request": "launch",
			"command": "wsl make iso",
			"terminalName": "F5 Anything",
			"terminalIndex": -1,
			"showTerminal": true,
			"vscCommand": ""
		}, {
			"name": "WSL - Clean",
			"type": "f5anything",
			"request": "launch",
			"command": "wsl make clean",
			"terminalName": "F5 Anything",
			"terminalIndex": -1,
			"showTerminal": true,
			"vscCommand": ""
		}, {
			"name": "Windows - Launch",
			"type": "f5anything",
			"request": "launch",
			"command": "$version = Get-Content -Raw -Path 'VERSION'; & qemu-system-x86_64 -cdrom \"./out/iso/AuraOS-$version.iso\"",
			"terminalName": "F5 Anything",
			"terminalIndex": -1,
			"showTerminal": true,
			"vscCommand": ""
		},
	]
}